      "\n",
      "def factorial( N ):\n",
      "    if N <= 1:\n",
      "        K = 1\n",
      "    else:\n",
      "        while K < N+1:\n",
      "            K = K * factorial(K-1)\n",
      "    return K\n",
      "\n",
      "print(factorial(2))"
